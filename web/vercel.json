{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "header",
          "key": "accept",
          "value": "text/html"
        },
        {
          "type": "header",
          "key": "sec-fetch-dest",
          "value": "document"
        }
      ],
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/:path*.worker.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}