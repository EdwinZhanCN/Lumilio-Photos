  },
  "pagination_examples": {
    "first_page": {
      "description": "第一页数据",
      "request": {
        "filter": {
          "type": "PHOTO"
        },
        "limit": 20,
        "offset": 0
      }
    },
    "second_page": {
      "description": "第二页数据",
      "request": {
        "filter": {
          "type": "PHOTO"
        },
        "limit": 20,
        "offset": 20
      }
    },
    "large_page": {
      "description": "大页面（最大限制）",
      "request": {
        "filter": {
          "type": "PHOTO"
        },
        "limit": 100,
        "offset": 0
      }
    }
  },
  "error_examples": {
    "invalid_search_type": {
      "description": "错误示例 - 无效的搜索类型",
      "endpoint": "POST /api/v1/assets/search",
      "request": {
        "query": "test",
        "search_type": "invalid_type",
        "limit": 10
      },
      "expected_error": {
        "code": 400,
        "message": "Invalid request parameters",
        "error": "search_type must be 'filename' or 'semantic'"
      }
    },
    "missing_query": {
      "description": "错误示例 - 缺少查询参数",
      "endpoint": "POST /api/v1/assets/search",
      "request": {
        "search_type": "filename",
        "limit": 10
      },
      "expected_error": {
        "code": 400,
        "message": "Invalid request data",
        "error": "Key: 'SearchAssetsRequest.Query' Error:Field validation for 'Query' failed on the 'required' tag"
      }
    },
    "invalid_rating": {
      "description": "错误示例 - 无效的评分值",
      "endpoint": "POST /api/v1/assets/filter",
      "request": {
        "filter": {
          "rating": 10
        },
        "limit": 20
      },
      "expected_error": {
        "code": 400,
        "message": "Invalid request data",
        "error": "rating must be between 0 and 5"
      }
    },
    "exceeds_limit": {
      "description": "错误示例 - 超过限制的页面大小",
      "endpoint": "POST /api/v1/assets/filter",
      "request": {
        "filter": {
          "type": "PHOTO"
        },
        "limit": 200
      },
      "note": "系统会自动调整为最大允许值100"
    }
  },
  "use_cases": {
    "photographer_workflow": {
      "description": "摄影师工作流程 - 查找需要后期处理的RAW文件",
      "steps": [
        {
          "step": 1,
          "action": "获取所有未评分的RAW文件",
          "endpoint": "POST /api/v1/assets/filter",
          "request": {
            "filter": {
              "type": "PHOTO",
              "raw": true,
              "rating": 0
            },
            "limit": 50
          }
        },
        {
          "step": 2,
          "action": "在特定日期范围内搜索风景照片",
          "endpoint": "POST /api/v1/assets/search",
          "request": {
            "query": "landscape nature mountains",
            "search_type": "semantic",
            "filter": {
              "type": "PHOTO",
              "raw": true,
              "date": {
                "from": "2024-09-01T00:00:00Z",
                "to": "2024-09-30T23:59:59Z"
              }
            },
            "limit": 30
          }
        }
      ]
    },
    "client_portfolio_selection": {
      "description": "客户作品集选择 - 为客户筛选最佳照片",
      "steps": [
        {
          "step": 1,
          "action": "获取所有5星评分的已点赞照片",
          "endpoint": "POST /api/v1/assets/filter",
          "request": {
            "filter": {
              "type": "PHOTO",
              "rating": 5,
              "liked": true
            },
            "limit": 100
          }
        },
        {
          "step": 2,
          "action": "在高质量照片中搜索人像作品",
          "endpoint": "POST /api/v1/assets/search",
          "request": {
            "query": "portrait people face wedding",
            "search_type": "semantic",
            "filter": {
              "rating": 5,
              "liked": true
            },
            "limit": 50
          }
        }
      ]
    },
    "equipment_analysis": {
      "description": "器材分析 - 分析不同设备的拍摄效果",
      "steps": [
        {
          "step": 1,
          "action": "获取所有可用的相机品牌和镜头",
          "endpoint": "GET /api/v1/assets/filter-options"
        },
        {
          "step": 2,
          "action": "比较Canon和Nikon的高评分作品",
          "endpoints": [
            {
              "camera": "Canon",
              "endpoint": "POST /api/v1/assets/filter",
              "request": {
                "filter": {
                  "type": "PHOTO",
                  "camera_make": "Canon",
                  "rating": 5
                },
                "limit": 50
              }
            },
            {
              "camera": "Nikon",
              "endpoint": "POST /api/v1/assets/filter",
              "request": {
                "filter": {
                  "type": "PHOTO",
                  "camera_make": "Nikon",
                  "rating": 5
                },
                "limit": 50
              }
            }
          ]
        }
      ]
    }
  }
}
